/* login registration */
#login_registration {
	float: right; 
	height: 102px;
}
#login_registration .register_box {
	float: left; 
	width: 138px;
}
	#login_registration .register_box h2 {
		height: 30px;
		padding-top: 15px;
		font-size: 11px; color: #fec92d;
		background: url(../images/login_reg_down_arrow.gif) no-repeat 20px bottom;
	}
	#login_registration .register_box a {
		display: inline-block;
		height: 32px;
		margin-top: 10px;
		padding-right: 40px;
		background: url(../images/register_link_arrow.gif) no-repeat right center;
		font-size: 10px; color: #fff; font-weight: bold;
		line-height: 32px; text-decoration: none;
	}

#login_registration .login_box {
	float: left; 
	width: 360px;
}
	#login_registration .login_box h2 {
		height: 30px;
		padding-top: 15px;
		font-size: 11px; color: #fec92d;
		background: url(../images/login_reg_down_arrow.gif) no-repeat 20px bottom;
	}
	#login_registration .login_box li {
		float: left;
		margin-right: 8px;
	}
	#login_registration .login_box li.input_fields {
		width: 180px;
	}
	#login_registration .login_box li.input_fields label {
		display: inline-block;
		margin: 3px 0;
		padding-right: 5px;
		text-align: right; line-height: 17px;
    height: 17px;
}
	#login_registration .login_box li.input_fields input.textfield {
		width: 120px;
		margin: 3px 0;
		padding: 2px;
		border-top: 1px solid #7e7b76;
		border-right: 1px solid #fff;
		border-bottom: 1px solid #fff;
		border-left: 1px solid #7e7b76;
		background: #fff url(../images/login_inputs_bg.gif) repeat-x 0 0;
	}
	#login_registration .login_box .login_button {
		display: block;
		width: 60px; height: 24px;
		margin-top: 18px;
		padding: 0 0 5px 0;
		border-style: none;
		background: url(../images/login_button.gif) no-repeat 0 0;
		font-size: 11px; color: #c9c9c9; font-weight: bold; line-height: 22px;
		cursor: pointer;
	}
	
	#login_registration .logout_box .login_button {
        display: block;
        width: 60px; height: 24px;
        margin-top: 15px;
        padding: 0 0 5px 0;
        border-style: none;
        background: url(../images/login_button.gif) no-repeat 0 0;
        font-size: 11px; color: #c9c9c9; font-weight: bold; line-height: 22px;
        cursor: pointer;
    }
	
	#login_registration .login_box a.forgot_pass {
		display: inline-block;
		padding-top: 20px;
		font-weight: bold; font-size: 10px;
	}

