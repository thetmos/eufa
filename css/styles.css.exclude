html, body, address, blockquote, div, dl, form, h1, h2, h3, h4, h5, h6, ol, p, pre, table, ul,
dd, dt, li, tbody, td, tfoot, th, thead, tr, button, del, ins, map, object,
a, abbr, acronym, b, bdo, big, br, cite, code, dfn, em, i, img, kbd, q, samp, small, span,
strong, sub, sup, tt, var, legend, fieldset {
	margin: 0;
	padding: 0;
}
img, fieldset {
	border: 0;
}
body, select, input, textarea {
	font-family: Tahoma, Arial Unicode;
	font-size: 11px; 
}
body {
	color: #fff;
}
ul, ol {
	list-style: none;
}
a {
	outline-style: none;
	color: #fff;
}
.breaker {
	clear: both;
	overflow: hidden;
}

body {
	background: #192a3b url(../images/body_background.png) repeat-x 0 0;
}


#page_wrapper {
	width: 1004px; 
	margin: 0 auto;
}
#page_footer {
	height: 97px; 
	background-color: #0a1522;
}
#page_footer .content {
	width: 1004px; height: 39px;
