#header_bottom {
	height: 34px;
	background: url(../images/top_menu_bg.png) repeat-x 0 bottom;
}

/* language bar */
#header_bottom ul.language_bar {
	float: right;
	height: 34px;
	background: url(../images/top_menu_right_bg.png) no-repeat right bottom;
}
#header_bottom ul.language_bar li {
	float: left;
	padding: 10px 5px 0;
}
	#header_bottom ul.language_bar li a {
		display: block;
		height: 16px;
		padding: 0 5px;
		text-decoration: none; font-size: 10px; font-weight: bold;
		line-height: 16px;
	}
	#header_bottom ul.language_bar li.selected a {
		background: url(../images/language_bar_selected.gif) repeat-x 0 0;
		border: 1px solid #b3b3b3;
		color: #000;
	}

/* top menu */
#header_bottom ul.top_menu {
	float: left; 
	height: 34px; padding: 0 25px;
	background: url(../images/top_menu_left_bg.png) no-repeat 0 bottom;
}
#header_bottom ul.top_menu li {
	float: left; 
	height: 32px;
	margin: 0 5px;
	background: url(../images/top_menu_item_bg.png) repeat-x left bottom;
}
#header_bottom ul.top_menu li.selected {
	background: url(../images/top_menu_selected_item_bg.png) repeat-x left bottom;
}
#header_bottom ul.top_menu li a {
	display: block;
	height: 32px;
	padding: 0 30px;
	font-size: 13px; font-weight: bold; line-height: 36px; color: #000; text-decoration: none;
}
#header_bottom ul.top_menu li.live a {
	padding: 0 15px 0 3px;
}
#header_bottom ul.top_menu li.live a strong {
	position: relative;
	float: left;
	width: 59px; height: 32px;
	background: url(../images/live_icon.gif) no-repeat 0 top;
	text-indent: -10000px;
}
#header_bottom ul.top_menu li.selected a strong {
	background-position: 0 bottom;
}



/* top actions bar */
#top_actions_bar {
	height: 37px;
	background: url(../images/top_actions_bar_bg.gif) repeat-x 0 0;
	border-bottom: 2px solid #feca31;
}
	/* top events */
	#top_actions_bar .events_header_container {
		float: left;
		width: 240px;
	}
	#top_actions_bar .events_header_container h2 {
		width: 95px; height: 18px;
		margin: 6px; padding: 3px 10px;
		background: url(../images/events_header_bg.gif) no-repeat 0 0;
		font-size: 13px; color: #c9c9c9;
	}
	#top_actions_bar .top_events_filter {
		float: left; 
		width: 432px;
		padding: 11px 6px;
	}
	
	
	/* top coupon */
	#top_actions_bar .coupon_container {
		position: relative;
		float: right;
		width: 264px; height: 24px;
	}
		#top_actions_bar .coupon_header_container {
			margin: 6px 0;
			background: url(../images/coupon_header_bg.gif) no-repeat 0 0;
		}
			#top_actions_bar .coupon_header_container h2 {
				padding: 3px 10px;
				font-size: 13px; color: #c9c9c9;
				background: transparent url(../images/coupon_arrows.gif) no-repeat 240px top;
				cursor: pointer;
			}
			#top_actions_bar .coupon_header_container h2.opened {
				background-position: 240px bottom;
			}
		
		#top_actions_bar .coupon_content_container {
			display: none;
			position: absolute; top: 39px; left: 0; z-index: 10000;
			width: 264px; 
			background-color: #feca31;
		}
			#top_actions_bar .coupon_content_container img.coupon_content_bg {
				display: none;
				position: absolute; top: 0; left: 0;
			}
			#top_actions_bar .coupon_content_container .coupon_content {
				position: relative;
			}
				#top_actions_bar .coupon_content_container .coupon_content .coupon_actions {
					padding: 15px;
					background-color: #101f2f;
					border-bottom: 4px solid #feca31;
					color: #fff;
				}
				#top_actions_bar .coupon_content_container .coupon_content .coupon_actions label {
					display: block;
					height: 20px;
					line-height: 18px;
				}
				#top_actions_bar .coupon_content_container .coupon_content .coupon_actions .textfield {
					float: right;
					width: 60px;
					padding: 2px;
					border-top: 1px solid #7e7b76;
					border-right: 1px solid #fff;
					border-bottom: 1px solid #fff;
					border-left: 1px solid #7e7b76;
					background: #fff url(../images/login_inputs_bg.gif) repeat-x 0 0;
				}
				#top_actions_bar .coupon_content_container .coupon_content .coupon_actions .possible_winning {
					display: block;
					line-height: 28px;
					font-weight: bold;
				}
					#top_actions_bar .coupon_content_container .possible_winning span {
						float: right;
					}
				#top_actions_bar .coupon_content_container .next_button {
					display: block;
					width: 70px; height: 24px;
					margin: 10px auto 0;
					padding: 0px 0 5px 0;
					border-style: none;
					background: url(../images/coupon_action_button.gif) no-repeat 0 0;
					font-size: 11px; color: #c9c9c9; font-weight: bold; line-height: 24px;
					cursor: pointer;
				}

				
				
				#top_actions_bar .coupon_content_container .coupon_content ul {
					height: 25px;
				}
					#top_actions_bar .coupon_content_container .coupon_content ul li {
						float: left;
						width: 50%; height: 25px;
						background-color: #4b5662;
						text-align: center; line-height: 25px; color: #d9d9d9; font-weight: bold;
						cursor: pointer;
					}
					#top_actions_bar .coupon_content_container .coupon_content ul li.selected {
						background-color: transparent;
						color: #121212;
					}
					#top_actions_bar .coupon_content_container .coupon_content .coupon_item_content {
						min-height: 100px;
					}
					#top_actions_bar .coupon_content_container .coupon_content #coupon_item_express_content,
					#top_actions_bar .coupon_content_container .coupon_content #coupon_item_system_content {
						display: none;
					}
					#top_actions_bar .coupon_content_container .coupon_content .item_blank {
						padding: 20px 10px;
						color: #222222;
					}
					#top_actions_bar .coupon_content_container .coupon_content .item {
						position: relative;
						padding: 10px;
						border-bottom: 2px solid #4c4c4c;
						color: #222222;
					}
						#top_actions_bar .coupon_content_container .coupon_content .item .remove {
							position: absolute; top: 5px; right: 5px;
							width: 11px; height: 11px;
							background: transparent url(../images/coupon_item_remove_icon.gif) no-repeat 0 0;
							cursor: pointer;
						}
						#top_actions_bar .coupon_content_container .coupon_content .item input.checkbox {
							vertical-align: top;
						}
						#top_actions_bar .coupon_content_container .coupon_content .item label {
							line-height: 18px;
							cursor: pointer;
						}
						#top_actions_bar .coupon_content_container .coupon_content .item div.selection {
							padding-left: 24px;
						}
							#top_actions_bar .coupon_content .item div.selection span {
								float: right;
							}
	/* top current balance */
	#top_actions_bar .top_current_balance {
		float: left;
